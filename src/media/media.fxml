<AnchorPane prefHeight="579.0" prefWidth="574.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="media.Controller">
   <children>
      <Button fx:id="btnPause" layoutX="195.0" layoutY="407.0" mnemonicParsing="false" onAction="#stopFunc" prefHeight="22.0" prefWidth="69.0" text="정지" />
